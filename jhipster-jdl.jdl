
entity Paciente {
	id Long
	nome String
    idade Integer
    email String
    telefone String
    whatsapp String
    pacienteDesde Instant
    ativo Boolean
    arquivos String
}
// Set pagination options
paginate Paciente with pagination

// Use Data Transfer Objects (DTO)
// dto * with mapstruct

// Set service options to all except few
service all with serviceImpl

